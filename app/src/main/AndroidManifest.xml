<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ps">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PS">
        <activity
            android:name=".LoginEmp"
            android:exported="false"
            android:label="@string/title_activity_login_emp"
            android:theme="@style/Theme.PS.NoActionBar" />
        <activity
            android:name=".LoginCli"
            android:exported="false"
            android:label="@string/title_activity_login_cli"
            android:theme="@style/Theme.PS.NoActionBar" />
        <activity
